<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Rent">
	
	<!-- 차 리스트 가져오기 -->
	<select id="carList" resultType="car">
		select * from c_car
	</select>
	
	<!-- 차 중간 저장 -->
	<insert id="insert_load">
		insert into c_rental(cr_num, cr_sdate, cr_edate, cr_rdate)
		values (cr_seq.nextval, #{cr_sdate}, #{cr_edate}, #{cr_rdate})
	</insert>
	
</mapper>